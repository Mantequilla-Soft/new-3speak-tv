# 3Speak Frontend Environment Variables
# Copy this file to .env and fill in the values

# ===========================================
# REQUIRED - Core API Configuration
# ===========================================

# Main 3Speak API endpoint (Western server)
VITE_API_URL_FROM_WEST=

# GraphQL API endpoint for video queries
VITE_GRAPHQL_API_URL=

# Video CDN domain for streaming
VITE_APP_VIDEO_CDN_DOMAIN=

# ===========================================
# REQUIRED - Upload Configuration
# ===========================================

# Authentication token for video uploads
VITE_UPLOAD_TOKEN=

# Upload endpoint URL
VITE_UPLOAD_URL=

# ===========================================
# OPTIONAL - JW Player Configuration
# ===========================================

# JW Player license credentials (optional - fallback player available)
VITE_JWPLAYER_LICENSE_ID=
VITE_JWPLAYER_LICENSE_KEY=

# ===========================================
# OPTIONAL - Development Overrides
# ===========================================

# Override Hive account for testing (not recommended for production)
# VITE_HIVE_USERNAME=
# VITE_HIVE_POSTING_KEY=
